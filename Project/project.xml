          <OnExamine />
          <OnGaze>game.ShowUse();</OnGaze>
          <OnUse>var dial = game.GetVariable("dial");
if (dial == 1) {
  dial = 2;
  game.ChangeBackground("Examine_Intercom", "intercom_2");
} else if (dial == 2) {
  dial = 3;
  game.ChangeBackground("Examine_Intercom", "intercom_3");
} else {
  dial = 1;
  game.ChangeBackground("Examine_Intercom", "intercom_1");
}
game.SetVariable("dial", dial);
game.PlaySound("klack");</OnUse>
